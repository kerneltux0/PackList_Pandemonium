<h1><center>Showing list: <%= @list.name %></center></h1>
<a href='/home'>Return Home</a>
<br>
<br>
<br>
<ul>
  <%@list.items.each do |item|%>
    <li><%= item.name.gsub(/_/,' ').capitalize! %></li>
  <%end%>
</ul>
<br>
<br>
<p><a href='/list/<%=@list.id%>/edit'>Edit this list!</a></p>

<p><a href='/list/<%=@list.id%>/delete'>Delete this list!</a></p>